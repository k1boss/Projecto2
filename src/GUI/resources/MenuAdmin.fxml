<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.controllers.MenuAdminController">


   <children>
      <GridPane prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="estabelecimentoBtn" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#adminestabelecimentoBtnAction" text="Estabelecimentos" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
            <Button fx:id="mesaBtn" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#adminmesaBtnAction" text="Mesas" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
            <Button fx:id="produtosBtn" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#adminprodutosBtnAction" text="Produtos" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
    
            <Button fx:id="logoutBtn" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#adminlogoutBtnAction" text="Logout" GridPane.halignment="CENTER" GridPane.rowIndex="6" />
            <Label fx:id="usernameLabel" text="Label" GridPane.halignment="RIGHT">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
               <padding>
                  <Insets right="20.0" />
               </padding>
            </Label>
         </children>
      </GridPane>
   </children>
</AnchorPane>
