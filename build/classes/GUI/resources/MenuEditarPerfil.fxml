<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.controllers.MenuEditarPerfilController">
   <children>
      <GridPane prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="mudarUsernameBtn" maxHeight="50.0" maxWidth="150.0" mnemonicParsing="false" onAction="#mudarUsername" text="Mudar Username" GridPane.halignment="CENTER" />
            <Button fx:id="mudarNomeBtn" maxHeight="50.0" maxWidth="150.0" mnemonicParsing="false" onAction="#mudarNome" text="Mudar Nome" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
            <Button fx:id="mudarPasswordBtn" maxHeight="50.0" maxWidth="150.0" mnemonicParsing="false" onAction="#mudarPassword" text="Mudar Password" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
            <Button fx:id="mudarEmailBtn" maxHeight="50.0" maxWidth="150.0" mnemonicParsing="false" text="Mudar Email" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
            <Button fx:id="voltarBtn" maxHeight="50.0" maxWidth="100.0" mnemonicParsing="false" onAction="#voltarBtnAction" text="Voltar" GridPane.halignment="CENTER" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
