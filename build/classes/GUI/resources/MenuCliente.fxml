<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="GUI.controllers.MenuClienteController">
   <children>
      <GridPane prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="escolherEstabelecimentoBtn" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#escolherEstabelecimentoBtnAction" text="Escolher Estabelecimento" GridPane.halignment="CENTER" />
            <Button fx:id="escolherMesaBtn" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#escolherMesaBtnAction" text="Escolher Mesa" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
            <Button fx:id="escolherProdutosBtn" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#escolherProdutosBtnAction" text="Escolher Produtos" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
            <Button fx:id="checkoutBtn" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#checkoutBtnAction" text="Checkout" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
            <Button fx:id="editarPerfilBtn" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#editarPerfilBtnAction" text="Editar Perfil" GridPane.halignment="CENTER" GridPane.rowIndex="4" />
            <Button fx:id="logoutBtn" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#logoutBtnAction" text="Logout" GridPane.halignment="CENTER" GridPane.rowIndex="5" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
